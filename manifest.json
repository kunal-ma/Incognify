{
	"manifest_version": 3,
	"name": "Incognify",
	"version": "1.0.0",

	"action": {
		"default_icon": {
			"16": "static/16.png",
			"24": "static/24.png",
			"32": "static/32.png"
		},
		"default_title": "Incognify"
	},
	"description": "Open current tab in its alternate state (normal or incognito)",
	"icons": {
		"16": "static/16.png",
		"32": "static/32.png",
		"48": "static/48.png",
		"128": "static/128.png"
	},	
	"permissions": ["tabs"],
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	}
}
